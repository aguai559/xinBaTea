/* pages/me/me.wxss */
@import '/icon/icon.wxss';
.container{
  /* background: #ccc; */
  height: 100vh;
  width: 100%;
  background: rgb(243,244,245);
}
.back{
  width: 100%;
  height: 35vh;
  /* background: pink; */
}
.user{
  width: 90%;
  background: white;
  /* text-align: center; */
  position: absolute;
  left: 50%;
  transform: translate(-50%,-50%);
  top: 40%;
  height: 20vh;
  /* margin-bottom: 20rpx; */
  border-radius: 25rpx;
  padding: 20rpx;
  box-sizing: border-box;
}
.head{
  position: absolute;
  left: 80rpx;
  top: -50rpx;
}
.headele{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  box-shadow: 5rpx 5rpx 5rpx #999;
  
}
.name{
  font-weight: bold;
}
.rank{
  position: absolute;
  left: 220rpx;
  top: 10rpx;
  width: 80rpx;
  height: 40rpx;
  background: white;
  border-radius: 50rpx;
  box-shadow: 2rpx 2rpx 2rpx #999;
  text-align: center;
}
.vip{
  width: 20rpx;
  height: 25rpx;
}
.rankNum{
  font-size: 25rpx;
  color: rgb(255,198,30);
}
.welcome{
  position: absolute;
  top:13vh ;
  display: inline-block;
  width: 400rpx;
  border-top: 4rpx solid rgb(255,198,30);
  font-size: 36rpx;
}
.bottom{
  position: absolute;
  /* background: rgb(255,198,30); */
  top: 55%;
  /* height: 20vh; */
  width: 100%;
  /* display: flex;
  justify-content: space-around; */
}
.all{
  position: absolute;
  left: 0;
  top: 80%;
  width: 100%;
  line-height: 16vh;
  height: 16vh;
}
.phone,.admin,.feedback{
  width: 100%;
  line-height: 8vh;
  height: 8vh;
  background: white;
  display: flex;
  border-bottom: 1px solid #999;
  /* background: white;
  width: 160rpx;
  display: inline-block; */
}
